<script type="642a87f48afde698e195a027-text/javascript">


var scrollOffsetTop = $(window).scrollTop(); // global scroll tracking variable.
var headerHeight = $(".www-mp-header .www-mp-titletray").height(); // global header height var
MP_ScrollTracker();


function MP_ScrollTracker() {
    $(window).scroll(function() {
        scrollOffsetTop = $(this).scrollTop();
    });
}

function MP_AnimateHomePosts() {
    setTimeout( function() {
        // Animate in News posts on scroll
        var $animation_elements = $('.article');
        var $window = $(window);

        $window.on('scroll resize', check_if_in_view);
        $window.trigger('scroll');


        function check_if_in_view() {
        var window_height = $window.height();
        var window_top_position = $window.scrollTop();
        var window_bottom_position = (window_top_position + window_height);

        $.each($animation_elements, function() {
            var $element = $(this);
            var element_height = $element.outerHeight();
            var element_top_position = $element.offset().top;
            var element_bottom_position = (element_top_position + element_height);

            //check to see if this current container is within viewport
            if ((element_bottom_position >= window_top_position) &&
                (element_top_position <= window_bottom_position)) {
                    //Apply class that triggers CSS animation if within viewport
            $element.addClass('news-viewed');
            }
        });
        }
    }, 1500);

}

function MP_ScrollBackToTop() {
    $('button.footer_ClickScrollUp').click(function(){
            $('html,body').animate({ scrollTop: 0 }, 'slow');
    });
}

function MP_ThreadActionTooltip() {

// Deals with showing description popups like/reply on hover

    $( ".publicControls .LikeLink" ).hover(
        // Custom Tooltips Reply and Linke
        function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").css( "display", "block" );
        }, function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").css( "display", "none" );
        }
    );

    $( ".publicControls .ReplyQuote" ).hover(
        // Custom Tooltips Reply and Linke
        function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipReply").css( "display", "block" );
        }, function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipReply").css( "display", "none" );
        }
    );

    // like
    $( ".publicControls .LikeLink" ).hover(
        function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").css( "display", "block" );
        }, function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").css( "display", "none" );
        }
    );

    // reply
    $( ".publicControls .ReplyQuote" ).hover(
        function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipReply").css( "display", "block" );
        }, function() {
            $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipReply").css( "display", "none" );
        }
    );


    $( "a.LikeLink.item.control.like" ).click(function() {
        //toggle like unlike tooltip on click
        $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").children("span.publicControls_TooltipLike_state").replaceWith( '<span class="publicControls_TooltipLike_state">Unlike</span>' );
    });

    $( "a.LikeLink.item.control.unlike" ).click(function() {
        //toggle like unlike tooltip on click
        $( this ).siblings(".publicControls_Tooltip.publicControls_TooltipLike").children("span.publicControls_TooltipLike_state").replaceWith( '<span class="publicControls_TooltipLike_state">Like</span>' );
    });

}

function MP_PostMessageDropdown() {

    // Toggles Thread Post Message Dropdown Menu
    var postOptOverlay = $(".message_MessageOptContainer div");

    $(".messageList").on('click', ".publicControls .MoreOptions", function () {
        //on click open the drop down by toggling class

        var postOpt = $( this );

        postOpt.toggleClass( "message_ShowOptions" );
        postOpt.siblings(".message_MessageOptionsPopup").toggleClass( "message_ShowOptions" );
        // also open an invisible background overlay to catch click off
        postOptOverlay.toggleClass( "message_MessageOptToggle" );


        $( postOptOverlay ).click(function() {

            // on click of invisible bg overlay close dropdown menu and remove bg overlay
            $( ".publicControls .MoreOptions" ).removeClass( "message_ShowOptions" );
            $( ".message_MessageOptionsPopup" ).removeClass( "message_ShowOptions" );
            postOptOverlay.removeClass( "message_MessageOptToggle" );


        });

        $(".messageList").on('click', ".message_MessageOptionsPopup.message_ShowOptions a", function () {
            // PATCH XENFORO CODE INTERACTING AND BREAKING THIS CODE
            // on click menu option open menu option & close dropdown menu and bg overlay to reset code
            $( ".publicControls .MoreOptions" ).removeClass( "message_ShowOptions" );
            $( ".message_MessageOptionsPopup" ).removeClass( "message_ShowOptions" );
            postOptOverlay.removeClass( "message_MessageOptToggle" );


        });


    });

}

function MP_UpdateNewForumReply() {
    $("body").on('click', "form.section.InlineMessageEditor input.button.primary", function () {
        // MP - Deals with duplicated forum post bug on post edit
        if (navigator.onLine) {
            // if the user is connected to the internet
            // aka can send post data, do the following
            // else, no point in running the following

            var editPostID = (($(this).closest("form.section.InlineMessageEditor").attr( 'action' )).slice(6, 30)).slice(0, -12) ;
            var editPostClass = ".post-"+editPostID;
            var editPostClassID = "post-"+editPostID;
            // get the ID of the post they're trying to edit

            $( document ).ajaxComplete(function() {
                // run this check each time ajax complete
                // since we can't use mutation observer because of xen code
                // but we can make out that each time an edit is attempting
                // to be made a few ajaxomplete events take place to add in
                // the data, and one of those events will add in the double
                // post we want to remove

                var allPostID = [];

                $( ".messageList .message" ).each(function() {
                    // get all the post IDs of all the forum posts currently
                    // on the page and push them to an array

                    if($(this).is('[class*="post-"]')) {
                        var ClassList =  $(this).attr('class');
                        var PostID = "post-"+(ClassList.split("post-").pop());
                        allPostID.push(PostID);
                    }
                });

                var allPostIDArray = allPostID.sort();
                // then sort this array
                var allPostIDDuplicate = [];

                for (var i = 0; i < allPostIDArray.length - 1; i++) {
                    // then go through every item in the array and check for
                    // duplicate occurences
                    if (allPostIDArray[i + 1] == allPostIDArray[i]) {
                        allPostIDDuplicate.push(allPostIDArray[i]);
                        // if a duplicate is found, push it into the
                        // duplicate holder array

                        if (navigator.onLine) {
                            // now run another check to see if user is still online
                            // after all this if online, safely hide all duplicate
                            // posts regardless of amount of edits done

                            $(editPostClass).css( "display", "none" );
                            $(editPostClass).first().css( "display", "block" );
                            var postDuplicateClass = ("."+allPostIDDuplicate);

                            var count = 0;
                            var count = (count + 1) % allPostIDDuplicate.length;

                            // this deals with duplication glitch outside
                            // of user's last post. set up a variable to go
                            // through every duplicate found currently in out array

                            if (editPostClassID != (allPostIDDuplicate[count])) {
                                // if we find a duplicate that is not the one we just
                                // hid for our current user, hide this duplicate as well
                                // and leave the most recent version

                                $("."+(allPostIDDuplicate[count])).css( "display", "none" );
                                $("."+(allPostIDDuplicate[count])).last().css( "display", "block" );
                            } else {
                            }

                        }
                    }
                }

            });

        }

    });
}

function MP_UpdateThreadonThreadAction() {

    function updateSidebar() {
        $("body").on('click', ".quickReply.message input.button.primary", function () {
            // MP - Updates sidebar with most recent post on thread after
            // submitting post

            $(document).ajaxSuccess(function( event, xhr, settings ) {
                var ajaxRegex = /^\/threads\/.+\/add-reply$/g,
                    Cpath = settings.url.slice((settings.url.indexOf(document.domain) + document.domain.length), settings.url.length);

                if (Cpath.match(ajaxRegex)) {
                    $('#widget-14 .WidgetFramework_WidgetRenderer_Threads_Sidebar.avatarList').load(document.URL +
                        ' #widget-14 .WidgetFramework_WidgetRenderer_Threads_Sidebar.avatarList ul');
                }

            });

        });

    }

    function preventDup() {
            $("body").on('click', ".quickReply.message input.button.primary", function () {
                // MP - Deals with duplicated forum post bug on post reply

                var currentUser = $('.quickReply.message .messageUserBlock div.avatarHolder .avatar img').attr("alt");
                var lastpost = $('.messageList .message').last();
                var lastpostUser = lastpost.find('div.avatarHolder .avatar img').attr("alt");
                var lastpostClassList = lastpost.attr('class');
                var lastpostIDClass = "post-"+(lastpostClassList.split("post-").pop());
                // find the latest post on the thread and the current user
                // and set as variables

                if (currentUser == lastpostUser) {
                    // if the last post on the thread was made by
                    // the current user, then..

                    $( document ).ajaxComplete(function() {
                    // run this check each time ajax complete
                    // since we can't use mutation observer because of xen code
                    // but we can make out that each time an edit is attempting
                    // to be made a few ajaxomplete events take place to add in
                    // the data, and one of those events will add in the double
                    // post we want to remove

                            var allPostID = [];

                            $( ".messageList .message" ).each(function() {
                                // get all the post IDs of all the forum posts currently
                                // on the page and push them to an array
                                if($(this).is('[class*="post-"]')) {
                                    var ClassList =  $(this).attr('class');
                                    var PostID = "post-"+(ClassList.split("post-").pop());
                                    allPostID.push(PostID);
                                }

                            });
                            var allPostIDArray = allPostID.sort();
                            // then sort this array
                            var allPostIDDuplicate = [];

                            for (var i = 0; i < allPostIDArray.length - 1; i++) {
                                if (allPostIDArray[i + 1] == allPostIDArray[i]) {
                                    // if we find a duplicate add it to the
                                    // duplicate array
                                    allPostIDDuplicate.push(allPostIDArray[i]);
                                    if (navigator.onLine) {
                                        // another check to make sure we're still online
                                        // if we are, safely hide the user's duplicated
                                        // last post and keep the updated post

                                        lastpost.css( "display", "none" );

                                        var count = 0;
                                        var count = (count + 1) % allPostIDDuplicate.length;

                                        // this deals with duplication glitch outside
                                        // of user's last post. set up a variable to go
                                        // through every duplicate found currently in out array


                                        if (lastpostIDClass != (allPostIDDuplicate[count])) {
                                            // if we find a duplicate that is not the one we just
                                            // hid for our current user, hide this duplicate as well
                                            // and leave the most recent version

                                            $("."+(allPostIDDuplicate[count])).css( "display", "none" );
                                            $("."+(allPostIDDuplicate[count])).last().css( "display", "block" );
                                        } else {

                                        }
                                    }
                                }
                            }

                    });

                }

            });

        }

    function preventDupEdit() {
        $("body").on('click', "form.section.InlineMessageEditor input.button.primary", function () {
            // MP - Deals with duplicated forum post bug on post edit
            if (navigator.onLine) {
                // if the user is connected to the internet
                // aka can send post data, do the following
                // else, no point in running the following

                var editPostID = (($(this).closest("form.section.InlineMessageEditor").attr( 'action' )).slice(6, 30)).slice(0, -12) ;
                var editPostClass = ".post-"+editPostID;
                var editPostClassID = "post-"+editPostID;
                // get the ID of the post they're trying to edit

                $( document ).ajaxComplete(function() {
                    // run this check each time ajax complete
                    // since we can't use mutation observer because of xen code
                    // but we can make out that each time an edit is attempting
                    // to be made a few ajaxomplete events take place to add in
                    // the data, and one of those events will add in the double
                    // post we want to remove

                    var allPostID = [];

                    $( ".messageList .message" ).each(function() {
                        // get all the post IDs of all the forum posts currently
                        // on the page and push them to an array

                        if($(this).is('[class*="post-"]')) {
                            var ClassList =  $(this).attr('class');
                            var PostID = "post-"+(ClassList.split("post-").pop());
                            allPostID.push(PostID);
                        }
                    });

                    var allPostIDArray = allPostID.sort();
                    // then sort this array
                    var allPostIDDuplicate = [];

                    for (var i = 0; i < allPostIDArray.length - 1; i++) {
                        // then go through every item in the array and check for
                        // duplicate occurences
                        if (allPostIDArray[i + 1] == allPostIDArray[i]) {
                            allPostIDDuplicate.push(allPostIDArray[i]);
                            // if a duplicate is found, push it into the
                            // duplicate holder array

                            if (navigator.onLine) {
                                // now run another check to see if user is still online
                                // after all this if online, safely hide all duplicate
                                // posts regardless of amount of edits done

                                $(editPostClass).css( "display", "none" );
                                $(editPostClass).first().css( "display", "block" );
                                var postDuplicateClass = ("."+allPostIDDuplicate);


                                var count = 0;
                                var count = (count + 1) % allPostIDDuplicate.length;

                                // this deals with duplication glitch outside
                                // of user's last post. set up a variable to go
                                // through every duplicate found currently in out array


                                if (editPostClassID != (allPostIDDuplicate[count])) {
                                    // if we find a duplicate that is not the one we just
                                    // hid for our current user, hide this duplicate as well
                                    // and leave the most recent version

                                    $("."+(allPostIDDuplicate[count])).css( "display", "none" );
                                    $("."+(allPostIDDuplicate[count])).last().css( "display", "block" );
                                } else {
                                }

                            }
                        }
                    }

                });

            }

        });

    }

    preventDup();
    updateSidebar();
    preventDupEdit();
}

function MP_postinfoNewPostsHover() {
    if( $(window).width() <= 840) return; // don't run for mobile.
    // Deals with New posts tooltips on hover

    // Only display post info tooltip after user
    // hovered for over specified time
    var delay = 700, setTimeoutConst;


    $( ".WidgetFramework_WidgetRenderer_Threads_Sidebar" ).on({
        mouseenter: function () {

        var postBlock = $( this ).children( ".NewPostsUserMessageSnippet" );
            //Follow on mouse move within every separate list element
            $( this ).on( "mousemove", function( event ) {
                // Account for page width, if not wide enough
                // display to right side of mouse
                if( (($(window).width()) - event.pageX ) >= 340) {

                $( postBlock ).css({
                "left" : ( event.pageX + 30),
                "top" : event.pageY
                });

                } else {

                $( postBlock ).css({
                "left" : ( event.pageX - 320),
                "top" : event.pageY
                });

                }
            });

        // Make the block show up after specified time
        setTimeoutConst = setTimeout(function(){
                $( postBlock ).css("display", "block");

            }, delay);

    },
    mouseleave: function () {

        var postBlock = $( this ).children( ".NewPostsUserMessageSnippet" );

        // Stop timer countdown
        clearTimeout(setTimeoutConst );

        // Hide block
        $( postBlock ).css("display", "none");

    }
    }, ".WidgetSidebarNewPosts");


}

function MP_StyleOversizedSignatures() {
    $( ".baseHtml.signature.messageText.ugc img" ).each(function() {
        // Style oversized banner images in signature

        var sigImage = $(this);
        var containerWidth = sigImage.closest(".baseHtml.signature.messageText.ugc").width();
        var imgWidth = sigImage.width();

        if (imgWidth >= (325)) {

            $(sigImage).css( "opacity", "0.6" );

        }

    });
}

function MP_FixCalendarAddonPositioning() {
    //fix event calendar addon forum thread text positioning
    $( ".message_MessageOptContainer" ).next("div").css( "margin-left", "20px" );
    $( ".message_MessageOptContainer" ).next("div").css( "margin-right", "20px" );
}

function MP_BulkModHandler() {
//fix event calendar addon forum thread text positioning
    $( ".message_MessageOptContainer" ).next("div").css( "margin-left", "20px" );
    $( ".message_MessageOptContainer" ).next("div").css( "margin-right", "20px" );

    if ($("#content").hasClass("forum_view")) {
    // button to toggle bulk moderation check boxes on thread list
        if ( $(".MP_UserCanInlineMod ").first().hasClass("MP_UserHasPermission")) {
            $(".MP_WatchForum").after("<a class='www-mp_togglebulkMod'>Bulk Moderation</a>");
        }

        $('.pageNavLinkGroup .linkGroup .www-mp_togglebulkMod').click(function() {
            $(".forum_view ol.discussionListItems h3.title input").toggleClass('showModerationCheckbox');
        });
    }

    if ($("#content").hasClass("thread_view")) {
    // button to toggle bulk moderation check boxes on threads
        if ( $(".MP_UserCanInlineMod ").first().hasClass("MP_UserHasPermission")) {
            $(".MP_ThreadTools").parent(".Popup").after(" <a class='www-mp_togglebulkMod'>Bulk Moderation</a>");
        }

        $('.linkGroup.SelectionCountContainer .www-mp_togglebulkMod').click(function() {
            $(".message .privateControls input").toggleClass('showModerationCheckbox');
        });
    }
}

function MP_LoadGoogleDocResponsive() {
    // Responsive Google Doc Mineplex
    if( $(window).width() < 660) {

        var mpDoc = $("#wwwMpTabs-3 div iframe");
        $(mpDoc).removeAttr("style");
        $(mpDoc).css({"width": "160vw", "transform": "scale(0.6)", "transform-origin": "0 0", "height": "14500px", "position": "relative"});
        $(mpDoc).parent("div").css({"-webkit-overflow-scrolling": "touch", "overflow-y": "scroll", "height": "500px", "position": "relative", "position": "relative", "overflow-x": "hidden"  });
        $("#wwwMpTabs-3").css( "height", "650px");
    }
}

function MP_ProfileCommentsShowMoreImages() {
    // Deals with profile comments being too long
    // adding toggle show more
    // Pt 1: Deals with images

    var comment = $("#ProfilePostList li .messageInfo .comment"),
        maxheight = 150; // limit at which to crop comments

    $(comment).each(function() {

        if ($(this).find(".commentContent").length > 0){

            var commentH = 0,
                thisComment = $(this),
                commentText = thisComment.find("blockquote"),
                hasimage = false,
                content = $(thisComment).find(".commentContent");

            if (($(commentText).find('img').length) > 0) {
                // get height of comment
                // if the comment contains an image
                hasimage = true;
                commentH = $(commentText).height();

            } else {
                // This is handled below
            }

            if (commentH > maxheight) {
                // if the comment is too large

                if (hasimage) {
                    // if comment has image make the height
                    // of the cropped comment 300px
                    $(content).css({"overflow": "hidden", "max-height": "300px"});
                    $(content).after('<div class="mp-commentshowmore">Show More</div>');
                    // adds toggler
                } else {
                    // This is handled below
                }

            }


        }

    });
}

function MP_ProfilePostCommentsShowMoretext() {
// Deals with profile comments being too long
    // adding toggle show more
    // Pt 2: non-images
    // This is separate to deal with text-only comments
    // faster than just when all the images are done loading

    var comment = $("#ProfilePostList li .messageInfo .comment"),
        maxheight = 150; // limit at which to crop comments

    $(comment).each(function() {

        if ($(this).find(".commentContent").length > 0){


            var commentH = 0,
                thisComment = $(this),
                commentText = thisComment.find("blockquote"),
                hasimage = false,
                content = $(thisComment).find(".commentContent");

            if (($(commentText).find('img').length) > 0) {
                // This is handled above

            } else {
                // if the document doesn't contain image
                // get height of comment
                commentH = $(thisComment).height();
                hasimage = false;
            }

            if (commentH > maxheight) {
                // if the comment is too large

                if (hasimage) {
                    // This is handled above
                } else {
                    // if comment doesn't have image make
                    // height of cropped comment 150px
                    $(content).css({"overflow": "hidden", "max-height": "150px"});
                    $(content).after('<div class="mp-commentshowmore">Show More</div>');
                    // adds toggler
                }

            }

        }

    });


    $(comment).on('click', ".mp-commentshowmore", function () {
        // handles toggling the full comments

        var toggler = $(this),
            commentContent = toggler.siblings(".commentContent");

        if ($(toggler).html() == "Show More") {
            // toggle text on click
            $(toggler).html('Show Less');
        } else {
            $(toggler).html('Show More');
        }

        if (($(commentContent).find('img').length) > 0) {
            // toggle heights for comments containing images
            if ((commentContent.css("max-height")) == "300px") {
                commentContent.css({"overflow": "", "max-height": ""});
            } else {
                commentContent.css({"overflow": "hidden", "max-height": "300px"});
            }

        } else {
            // toggle heights for comments containing just text
            if ((commentContent.css("max-height")) == "150px") {
                commentContent.css({"overflow": "", "max-height": ""});
            } else {
                commentContent.css({"overflow": "hidden", "max-height": "150px"});
            }

        }


    });
}

function MP_ToggleLoginPopup() {
    $("#UserInformationHeader").on('click', "#loginBarHandle", function () {
    // Deals with login popup
        $(".pageWidth.loginPopupContainerMP").toggleClass("MPLogin_toggled");
        $("._swOuter").css("height", "");


        $("body").on('click', "#exposeMask", function () {
            $(".pageWidth.loginPopupContainerMP").removeClass("MPLogin_toggled");
            $("._swOuter").css("height", "0px");
        });

        $("body").on('click', ".MPLogin_ClosePopup", function () {
            $(".pageWidth.loginPopupContainerMP").removeClass("MPLogin_toggled");
            $("._swOuter").css("height", "0px");
        });


    });



    $("body").on('click', "#loginBarHandle_thread", function () {
    // Deals with login popup
        $(".pageWidth.loginPopupContainerMP").toggleClass("MPLogin_toggled");
        $("._swOuter").css("height", "");


        $("body").on('click', "#exposeMask", function () {
            $(".pageWidth.loginPopupContainerMP").removeClass("MPLogin_toggled");
            $("._swOuter").css("height", "0px");
        });

        $("body").on('click', ".MPLogin_ClosePopup", function () {
            $(".pageWidth.loginPopupContainerMP").removeClass("MPLogin_toggled");
            $("._swOuter").css("height", "0px");
        });


    });
}

function MP_ShowPlayerCountOnHover() {
    $( "li.mp-playercount.mp-playercounttotal" ).hover(
    function() {
        // On mouse over Player count
        $("li.mp-serverinfo.mp-serverip").css({"transition": "0.2s ease", "opacity": "0", "top": "30px"});
        $(".www-mp-server-tray li.mp-serverinfo.mp-playercount").css({"transition": "0.2s ease", "opacity": "1", "top": "0"});

    }, function() {
        // On Mouse leave Player count
        $("li.mp-serverinfo.mp-serverip").css({"opacity": "1", "top": "0"});
        $(".www-mp-server-tray li.mp-serverinfo.mp-playercount").css({"opacity": "0", "top": "-30px"});

    }
    );
}

function MP_ToggleHidenForumSection() {
    // Show and hide Forum Section
    $('.showForumSS').click(function() {
        $(this).closest('.node.category').find('.nodeList').toggleClass('hideThisList');
        $(this).children('i.far').toggleClass('hideEyeToggle');
    });
}

function MP_ParallaxHeader() {
    if( $(window).width() <= 840) return; // don't do this for mobile.

    // Animate Header Parallax
    var lFollowX = 0,
    lFollowY = 0,
    x = 0,
    y = 0,
    friction = 1 / 3,
    l1Speed = 0.05,
    l2Speed = 0.25,
    l3Speed = 1,
    l4Speed = 2,
    l5Speed = 0,
    parallaxBox = $(window),
    animationID,
    active = false;

    function moveBackground() {
        x += (lFollowX - x) * friction;
        y += (lFollowY - y) * friction;

        // Transform Layers using specified speed
        transformIt( '#l1', l1Speed, x, y );
        transformIt( '#l2', l2Speed, x, y );
        transformIt( '#l3', l3Speed, x, y );
        transformIt( '#l4', l4Speed, x, y );
        transformIt( '#l5', l5Speed, x, y );

        // Animate it
        animationID = requestAnimationFrame(moveBackground);
    }

    function transformIt ( id, tSpeed , xVal, yVal ) {
        // Apply transform translate 3d for all the custom layers using their custom values

        $( id ).css({
            '-webit-transform': 'translate(' + (( xVal * tSpeed ).toFixed(3)) + 'px, ' + (( yVal * tSpeed).toFixed(3)) + 'px) scale(1)',
            '-moz-transform': 'translate(' + (( xVal * tSpeed ).toFixed(3)) + 'px, ' + (( yVal * tSpeed).toFixed(3)) + 'px) scale(1)',
            'transform': 'translate(' + (( xVal * tSpeed ).toFixed(3)) + 'px, ' + (( yVal * tSpeed).toFixed(3)) + 'px) scale(1)'
        });
    }

    function bindMouseMove() {
        $(parallaxBox).on('mousemove click', function(e) {

            var lMouseX = Math.max(-1000, Math.min(1000, $(parallaxBox).width() / 2 - e.clientX));
            var lMouseY = Math.max(-1000, Math.min(1000, $(parallaxBox).height() / 2 - e.clientY));
            var MouseOffsetY = Math.max( (0 - $(window).height() ), Math.min( $(window).height() , $(parallaxBox).height() - e.clientY));

            // Only if mouse is positioned within the header
            if ( (MouseOffsetY - scrollOffsetTop) >= ($(window).height() - headerHeight) ) {
                if (scrollOffsetTop < headerHeight) {
                    lFollowX = (20 * lMouseX) / 400;
                    lFollowY = (2 * lMouseY) / 100;
                }
            }

        });
    }

    function unbindMouseMove() {
        $(parallaxBox).unbind("click");
        $(parallaxBox).unbind("mousemove");
    }


    if (scrollOffsetTop < headerHeight) {
        // initial function call, if the header is in view we will call the animation function
        bindMouseMove();
        animationID = requestAnimationFrame(moveBackground);
        active = true;
    }

    $(window).scroll(function() {
        // each time we scroll, we will re-evaluate the window position relative to the header position.
        // if it's in view after not being active, we will activate the animation.
        // if it is out of view after being activated, we will end the animation as well as unbind mouse tracking
        if (scrollOffsetTop < headerHeight) {
            // in view
            if (!active) {
                animationID = requestAnimationFrame(moveBackground);
                bindMouseMove();
                active = true;
            }
        } else {
            // not in view
            if (active) {
                // cancel animation
                cancelAnimationFrame(animationID);
                unbindMouseMove();
                active = false;
            }
        }
    });
}

function MP_StickyNav() {
    $(document).scroll(function () {
        //Sticky Header Animate in

        //Stick nav to top of page
        var y = $(this).scrollTop();

        //Set pixel value at which to display sticky nav
        var noDispHeight = $('body').offset().top + 300,
            noDispHeight2 = $('body').offset().top + 260;

        //But not for devices with a screen width smaller than 840 px
        if( $(window).width() > 840) {
            if (y > noDispHeight) {

                // Animate in  sticky header CSS3
                $('.www-mp-nav.mp-nav-sticky').addClass('navScrolled');
            } else {
                $('.www-mp-nav.mp-nav-sticky').removeClass('navScrolled');
            }
        }

        if( $(window).width() > 840) {
            if (y > noDispHeight2) {

                // Fallback for fast scroll, display none after specified point
                $('.www-mp-nav.mp-nav-sticky').removeClass('navNoDisplay');
            } else {
                $('.www-mp-nav.mp-nav-sticky').addClass('navNoDisplay');
            }
        }

    });
}

// Mobile


function MP_MobileMenuHandler() {
    var opened = false;

    function MP_MobileMenuHandler_ScrollOpener() {
        $( "li.navTab.ResponsiveMenu-mobile" ).click(function() {
            //responsive scroll to nav and open nav if clicked on nav icon

            //on click of responsive nav toggle button fixed sticky nav

                if ($(".www-mp-nav-item.responsive-menu").siblings(".www-mp-nav-item").css('display') == 'list-item') {
                // if the responsive nav is open..

                var scrolllimit = 100;


                    if ($(window).scrollTop() > scrolllimit) {
                    // if the user has scrolled away more than specified number
                    // pixels from the top of the window
                    // and clicks the responsive toggle button again
                    // simply scroll back up to the menu without toggling it

                        $([document.documentElement, document.body]).animate({
                            scrollTop: $("ul.www-mp-nav").offset().top
                        }, 1000);

                    } else {
                        // else the user most likely intends to close the
                        // responsive menu, so close the responsive menu
                        // by changing its css styles

                        MP_MobileMenu_close();
                    }

                } else {
                    // else if the responsive nav isn't open
                    // then the user intends to open it
                    // so we toggle the css classes to show it

                    MP_MobileMenu_open();

                    setTimeout( function() {
                        // and also scroll the user to the nav on click
                        $([document.documentElement, document.body]).animate({
                            scrollTop: $("ul.www-mp-nav").offset().top
                        }, 1000);

                    }, 100);
                }

        });
    }

    function MP_MobileMenu_open() {
        if (!opened) {
            $(".www-mp-nav-item.responsive-menu").siblings(".www-mp-nav-item").css( "display", "list-item" );
            opened = true;
        }
    }

    function MP_MobileMenu_close() {
        if (opened) {
            $(".www-mp-nav-item.responsive-menu").siblings(".www-mp-nav-item").css( "display", "none" );
            opened = false;
        }
    }

    MP_MobileMenuHandler_ScrollOpener();

    $(".responsive-menu .www-mp-nav-button").click(function() {
        if (!opened) {
            MP_MobileMenu_open();
        } else {
            MP_MobileMenu_close();
        }
    });

    $("div#content").click(function() {
        MP_MobileMenu_close();
    });

}


function MP_fixOffsetUserDropdownMobile() {
    if($(window).height() - $("li.navTab.account").height() > $("#AccountMenu").height()) return; // don't do this for large enough screens

    $("#AccountMenu").css("height", $(window).height() - $("li.navTab.account").height());
    $("#AccountMenu").css("overflow-y", "scroll");
}



function MP_OpenVisitorOwnStatsOnClick() {
    $("div#VisitorJavaChar").on("click", function() {
        var regexURL = /^(https:\/\/(?:www|xen)\.mineplex\.com\/)(?:.+)$/g;
        var currentURL = window.location.href;
        var domainString = currentURL.replace(regexURL, '$1');
        var finalUrl = domainString + "players/" + $(this).find("span#VisitorJavaCharacterName a").text();
        window.location.href = finalUrl;

    });
}


function MP_UpdateEditPrivateMessage() {
    // Duplicate message prevention for private messages on edit
    // Simplar to preventDupEdit in MP_UpdateThreadonThreadAction

    $(document).ajaxSuccess(function( event, xhr, settings ) {
        var ajaxRegex = /^\/conversations\/\w+\.\d+\/save-message\?m=(\d+)$/,
            Cpath = settings.url.slice((settings.url.indexOf(document.domain) + document.domain.length), settings.url.length);

        if (Cpath.match(ajaxRegex)) {

            var allPostID = [];

            $( ".messageList .message" ).each(function() {
                // get all the post IDs of all the forum posts currently
                // on the page and push them to an array
                if($(this).is('[class*="message-"]')) {
                    var ClassList =  $(this).attr('class');
                    var PostID = "message-"+(ClassList.split("message-").pop());
                    allPostID.push(PostID);
                }
            });

            var allPostIDArray = allPostID.sort();
            var allPostIDDuplicate = [];

            for (var i = 0; i < allPostIDArray.length - 1; i++) {
                // then go through every item in the array and check for
                // duplicate occurences
                if (allPostIDArray[i + 1] == allPostIDArray[i]) {
                    allPostIDDuplicate.push(allPostIDArray[i]);
                    // if a duplicate is found, push it into the
                    // duplicate holder array

                    if (navigator.onLine) {
                        // now run another check to see if user is still online
                        // after all this if online, safely hide all duplicate
                        // posts regardless of amount of edits done
                        var editPostClass = Cpath.replace(ajaxRegex, '$1');
                        editPostClass = "message-" + editPostClass;

                        $("." + editPostClass).css( "display", "none" );
                        $("." + editPostClass).first().css( "display", "block" );

                        var count = 0;
                        var count = (count + 1) % allPostIDDuplicate.length;

                        // this deals with duplication glitch outside
                        // of user's last post. set up a variable to go
                        // through every duplicate found currently in our array


                        if (editPostClass != (allPostIDDuplicate[count])) {
                            // if we find a duplicate that is not the one we just
                            // hid for our current user, hide this duplicate as well
                            // and leave the most recent version

                            $("."+(allPostIDDuplicate[count])).css( "display", "none" );
                            $("."+(allPostIDDuplicate[count])).last().css( "display", "block" );
                        } else {
                        }

                    }
                }
            }

        }

    });
}


// Now load in functions:

MP_ThreadActionTooltip();
MP_ShowPlayerCountOnHover();
MP_StickyNav();
MP_OpenVisitorOwnStatsOnClick();



function MP_HalloweenLights() {

    var intervalID = null;
    var heightBanner = $(".www-mp-titletray").height();

    setTimeout(MP_setupLights,400);
    MP_Halloween_onscrolltrack();

    function MP_setupLights() {
        for (var i = 0; i < 3; i++) {
            $(`#l3_${i+1}`).css("transition","0.5s ease");
        }
    }

    function MP_getRandomOpacity() {
        return  (Math.round(Math.random() * 100) / 100) + "";
    }

    function MP_UpdateLights() {
        for (var i = 0; i < 3; i++) {
            $(`#l3_${i+1}`).css("opacity",MP_getRandomOpacity());
        }
    }

    function MP_Halloween_onscrolltrack() {
        intervalID = setInterval(MP_UpdateLights, 500);

        $(window).scroll(function() {
            var topOffset = $(this).scrollTop();

            if (topOffset > heightBanner) {
                if (intervalID !== null) {
                    clearInterval(intervalID);
                    intervalID = null;
                }
            } else if (intervalID === null) {
                intervalID = setInterval(MP_UpdateLights, 500);
            }

        });
    }
}


$(window).on('load', function() {
    if ($(".www-mp-titletray").css("display") != "none") {
        // Only bother with the Header animation if the header is visible
        // Aka not intentionally "disabled" for some specific pages
        MP_ParallaxHeader();
    }
    MP_AnimateHomePosts();
    MP_ScrollBackToTop();
    MP_postinfoNewPostsHover();
    MP_StyleOversizedSignatures();
    MP_LoadGoogleDocResponsive();
    MP_ProfileCommentsShowMoreImages();
    MP_fixOffsetUserDropdownMobile();
    MP_HalloweenLights();
});


